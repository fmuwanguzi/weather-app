

<% weather.forEach(user => { %>
    <ul>
        <li><%= user.city %>, <%= user.country %> ,
             <%=user.description %>,<b>temp</b><%=user.temperature %>
            <form action="profile?_method=DELETE" method="POST">
            <input hidden type="text" name="city" id="city" value="<%=user.city%>">
            <input type="submit" value="remove saved">
            </form>
        </li> 
    </ul>
<% }); %> 



  


